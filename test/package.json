{
    "name": "sports-posture-analyzer-tests",
    "version": "1.0.0",
    "description": "Automated tests for Sports Posture Analyzer R2 upload functionality",
    "main": "test-r2-automation.js",
    "scripts": {
        "test": "node test-r2-automation.js",
        "test:config": "node test-r2-config.js",
        "test:with-file": "node test-r2-automation.js",
        "demo": "node demo-r2-flow.js",
        "start:backend": "cd ../backend && npm start",
        "start:frontend": "cd ../front && npm run dev",
        "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
        "install:all": "npm install && cd ../backend && npm install && cd ../front && npm install"
    },
    "dependencies": {
        "node-fetch": "^2.6.7",
        "form-data": "^4.0.0"
    },
    "devDependencies": {},
    "keywords": [
        "test",
        "automation",
        "r2",
        "cloudflare",
        "upload"
    ],
